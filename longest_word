def longest_word(s):
    words = s.split()

    max_length = 0
    longest = ""

    for word in words:

        if len(word) > max_length:
            max_length = len(word)
            longest = word

    return longest

text = "Съешь еще этих мягких французских булок да выпей чаю"
print(longest_word(text))
